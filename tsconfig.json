{
    "compilerOptions": {
        "outDir": "./dist/",
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "experimentalDecorators": true,
        "sourceMap": true,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
    },
    "include": [
        "./src/**/*", "src/client/.d.ts"
    ]
}
