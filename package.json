{
  "name": "boilerplate-react-node",
  "version": "1.0.0",
  "description": "React, Node, Express, Typescript, Babel, Webpack, Eslint, MaterialUI, SASS",
  "repository": "https://github.com/Isur/boilerplate-react-node.git",
  "author": "Isur",
  "license": "MIT",
  "scripts": {
    "test": "jest --coverage",
    "test_watch": "jest --watch --coverage",
    "precommit": "npm run linter-no-warning && npm run typecheck && npm run test",
    "predev": "npm run linter-fix & rm -rf dist/",
    "dev": "npm run dev:server & npm run dev:client",
    "dev:server": "NODE_ENV=development webpack --config ./webpack/webpack.config.server.babel.js",
    "dev:server-start": "NODE_ENV=development nodemon dist/server.bundle.js",
    "dev:client": "webpack-dev-server --config ./webpack/webpack.config.dev.babel.js",
    "linter": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx --ignore-path .gitignore ./src",
    "typecheck": "tsc --noEmit",
    "linter-fix": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx --color --ignore-path .gitignore ./src --fix",
    "linter-no-warning": "eslint --max-warnings 0 --ext .js --ext .jsx --ext .ts --ext .tsx --ignore-path .gitignore ./src",
    "prebuild": "npm run linter-no-warning",
    "build": "npm run build:clean && npm run build:server && npm run build:client",
    "build:clean": "rm -rf build/",
    "build:server": "webpack --config ./webpack/webpack.config.server.babel.js",
    "build:client": "webpack --config ./webpack/webpack.config.prod.babel.js",
    "dockerbuild": "npm run build:server &&  npm run build:client",
    "start": "node ./build/server.bundle.js"
  },
  "keywords": [
    "webpack",
    "typescript",
    "node",
    "react",
    "babel",
    "express",
    "sass",
    "material",
    "eslint"
  ],
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@babel/register": "^7.7.4",
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.17.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.2",
    "@types/express-promise-router": "^2.0.1",
    "@types/express-session": "^1.15.16",
    "@types/history": "^4.7.3",
    "@types/jest": "^24.0.23",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/lodash": "^4.14.149",
    "@types/pg": "^7.11.2",
    "@types/react": "^16.9.13",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-window": "^1.8.1",
    "@types/redux": "^3.6.0",
    "@types/redux-devtools-extension": "^2.13.2",
    "@types/uuid": "^3.4.6",
    "@types/redux-thunk": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.5",
    "css-loader": "^2.1.1",
    "eslint": "^6.7.2",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-react": "^7.17.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "node-sass": "^4.13.0",
    "nodemon": "^1.19.4",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.0",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-node-externals": "^1.7.2",
    "webpack-shell-plugin": "^0.5.0"
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.8.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/pickers": "^3.2.8",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chalk": "^2.4.2",
    "connected-react-router": "^6.6.1",
    "cookie-parser": "^1.4.4",
    "date-fns": "^2.0.0-beta.5",
    "express": "^4.16.4",
    "express-promise-router": "^3.0.3",
    "express-session": "^1.17.0",
    "express-validator": "^6.3.0",
    "history": "^4.10.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "pg": "^7.14.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hook-form": "^4.0.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "uuid": "^3.3.3"
  }
}
